{
  "mcpServers": {
    "my-test-mcp-server": {
      "disabled": false,
      "url": "http://127.0.0.1:9400/mcp",
      "type": "streamable-http"
    },
    "mcp-go-lsp": {
      "disabled": false,
      "command": "/home/msg/code/mcp_servers/mcp-language-server/mcp-language-server",
      "env": {
        "GOMODCACHE": "/home/msg/go/pkg/mod",
        "GOCACHE": "/home/msg/.cache/go-build",
        "GOPATH": "/home/msg/go",
        "LOG_LEVEL": "DEBUG"
      },
      "args": [
        "--workspace",
        "/home/msg/code/go_aws_lambda_webhook",
        "--lsp",
        "/home/msg/go/bin/gopls"
      ]
    },
    "brave-search": {
      "slash_commands": true,
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "disabled": true,
      "prompts": {
        "recent_docs": {
          "params": {
            "query": "recent documentation for code"
          },
          "tool": "search",
          "description": "Retrieve recent documentation related to code to assist the user using the brave search"
        }
      },
      "env": {
        "BRAVE_API_KEY": ""
      },
      "custom_instructions": {
        "disabled": false,
        "text": "search tool to get recent docs"
      }
    },
    "postgres": {
      "disabled": true,
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "postgresql://app_user:app_password@localhost:5432/vm"
      ]
    }
  },
  "nativeMCPServers": [
    {
      "context7": {
        "command": "npx",
        "args": [
          "-y",
          "@upstash/context7-mcp"
        ]
      }
    }
  ]
}